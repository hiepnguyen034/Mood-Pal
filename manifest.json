{
	"name": "Moodpal",
	"version": "1.0",
	"description": "A mood tracker",
	"browser_action":{
		"default_title": "Moodpal"	
	},
	"background":{
		"scripts": ["background.js"],
		"persistent": false
	},
	"permissions":[
		"tabs",
		"windows",
		"system"
	],
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' https://connect.facebook.net https://graph.facebook.com; object-src 'self'"
}
